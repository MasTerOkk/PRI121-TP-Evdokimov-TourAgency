@model agency.Models.Order
@foreach (var i in Model.OrderedTours)
{
    <div class="card">
        <div style ="margin-top: 30px; margin-left: 30px">
            <h4>@i.Tour.TourName</h4>
            <p>Количесво человек @i.PersonsCount</p>
            <p>@i.Tour.Price₽</p>
        </div>
        
    </div>
    
}

<h2>Итоговая цена - @Model.Price</h2>